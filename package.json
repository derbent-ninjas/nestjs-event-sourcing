    "fp-ts": "^2.16.9",
    "kafkajs": "^2.2.4",
    "lodash": "^4.17.21",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "0.3.20",
    "typeorm-transactional": "^0.5.0",
    "uuid": "^11.0.5",
    "uuid-by-string": "^4.0.0"
